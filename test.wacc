    fi;

    char[] cArrA = call returnCharArray();
    char[] cArrB = call returnCharArray();

    if cArrA == cArrB then
        println "houston, we really have a problem"
    else
        println "cool"
    fi;

    int[] iArrA = call returnIntArray();
    int[] iArrB = call returnIntArray();

    if iArrA == iArrB then
        println "houstong, what the heck?"
    else
        println "nice"
    fi
end
